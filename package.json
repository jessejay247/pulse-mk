{
  "name": "pulse_market_server",
  "version": "2.0.0",
  "description": "PulseMarkets API Server - Real-time forex, crypto, and stock data",
  "main": "api-server.js",
  "scripts": {
    "start": "node api-server.js",
    "start:ingestion": "node finnhub-app.js",
    "start:data-manager": "node scripts/data-manager.js",
    "dev": "nodemon api-server.js",
    "dev:ingestion": "nodemon finnhub-app.js",
    "seed": "node scripts/seed-historical.js",
    "seed:majors": "node scripts/seed-historical.js EURUSD GBPUSD USDJPY USDCHF AUDUSD USDCAD NZDUSD",
    "seed:metals": "node scripts/seed-historical.js XAUUSD XAGUSD",
    "seed:quick": "node scripts/seed-historical.js EURUSD GBPUSD XAUUSD --years 1",
    "import": "node import-historical.js",
    "import-batch": "node batch-import.js",
    "backfill": "node scripts/backfill-gaps.js",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "dukascopy-node": "^1.1.3",
    "express": "^4.18.2",
    "mysql2": "^3.6.3",
    "node-cron": "^3.0.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
