{
  "name": "pulse_market_server",
  "version": "3.0.0",
  "description": "PulseMarkets Self-Healing Forex Data Engine",
  "main": "api-server-v2.js",
  "scripts": {
    "start": "node server.js",
    "start:healer": "node smart-healing-engine.js",
    "start:healer:now": "node smart-healing-engine.js --heal-now",
    "dev": "nodemon api-server-v2.js",
    
    "seed": "node scripts/seed-historical.js",
    "seed:primary": "node scripts/seed-historical.js --primary --preset quick",
    "seed:primary:full": "node scripts/seed-historical.js --primary --preset full",
    "seed:secondary": "node scripts/seed-historical.js --secondary --preset quick",
    "seed:all": "node scripts/seed-historical.js --all --preset quick",
    "seed:all:full": "node scripts/seed-historical.js --all --preset full",
    
    "seed:m1:1m": "node scripts/seed-historical.js --primary --timeframe M1 --months 1",
    "seed:m5:3m": "node scripts/seed-historical.js --primary --timeframe M5 --months 3",
    "seed:h1:2y": "node scripts/seed-historical.js --primary --timeframe H1 --years 2",
    "seed:d1:10y": "node scripts/seed-historical.js --all --timeframe D1 --years 10",
    
    "nuclear": "node scripts/nuclear-fix.js",
    "nuclear:eurusd": "node scripts/nuclear-fix.js EURUSD --hours 24",
    "nuclear:primary": "node scripts/nuclear-fix.js --primary --hours 24",
    "nuclear:primary:3d": "node scripts/nuclear-fix.js --primary --days 3",
    "nuclear:all": "node scripts/nuclear-fix.js --all --hours 24",
    
    "fix": "node scripts/quick-fix-incomplete.js",
    "fix:eurusd": "node scripts/quick-fix-incomplete.js EURUSD --hours 24",
    "fix:primary": "node scripts/quick-fix-incomplete.js --hours 24",
    
    "ops": "node scripts/manual-operations.js",
    "ops:health": "node scripts/manual-operations.js health",
    "ops:gaps": "node scripts/manual-operations.js gaps --primary",
    "ops:gaps:fix": "node scripts/manual-operations.js gaps --primary --fix",
    "ops:verify": "node scripts/manual-operations.js verify",
    
    "cleanup": "node scripts/cleanup-closed-market.js",
    "cleanup:execute": "node scripts/cleanup-closed-market.js --execute",
    
    "legacy:start": "node api-server.js",
    "legacy:ingestion": "node finnhub-app.js",
    
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "dukascopy-node": "^1.1.3",
    "express": "^4.18.2",
    "mysql2": "^3.6.3",
    "node-cron": "^3.0.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "PulseMarkets",
  "license": "PROPRIETARY"
}