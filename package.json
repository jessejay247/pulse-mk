{
  "name": "pulse_market_server",
  "version": "3.0.0",
  "description": "PulseMarkets Self-Healing Forex Data Engine",
  "main": "api-server-v2.js",
  "scripts": {
    "start": "node api-server-v2.js",
    "start:engine": "node self-healing-engine.js",
    "dev": "nodemon api-server-v2.js",
    
    "ops": "node scripts/manual-operations.js",
    "ops:health": "node scripts/manual-operations.js health",
    "ops:gaps": "node scripts/manual-operations.js gaps --primary",
    "ops:gaps:fix": "node scripts/manual-operations.js gaps --primary --fix",
    
    "backfill": "node scripts/manual-operations.js backfill",
    "backfill:eurusd": "node scripts/manual-operations.js backfill EURUSD --days 7",
    "backfill:primary": "node scripts/manual-operations.js backfill EURUSD GBPUSD USDJPY XAUUSD --days 7",
    
    "verify": "node scripts/manual-operations.js verify",
    "verify:all": "node scripts/manual-operations.js verify EURUSD M1 --days 7",
    
    "fix": "node scripts/manual-operations.js fix-incomplete",
    "rebuild": "node scripts/manual-operations.js rebuild",
    
    "seed": "node scripts/seed-historical.js",
    "seed:majors": "node scripts/seed-historical.js EURUSD GBPUSD USDJPY USDCHF AUDUSD USDCAD NZDUSD --months 1",
    "seed:metals": "node scripts/seed-historical.js XAUUSD XAGUSD --months 1",
    "seed:primary": "node scripts/seed-historical.js EURUSD GBPUSD USDJPY XAUUSD USDCHF AUDUSD USDCAD NZDUSD EURGBP EURJPY GBPJPY --months 1",
    "seed:force": "node scripts/seed-historical.js --force --months 1",
    
    "cleanup": "node scripts/cleanup-closed-market.js",
    "cleanup:execute": "node scripts/cleanup-closed-market.js --execute",
    
    "legacy:start": "node api-server.js",
    "legacy:ingestion": "node finnhub-app.js",
    "legacy:data-manager": "node scripts/data-manager.js",
    
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "dukascopy-node": "^1.1.3",
    "express": "^4.18.2",
    "mysql2": "^3.6.3",
    "node-cron": "^3.0.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "PulseMarkets",
  "license": "PROPRIETARY"
}